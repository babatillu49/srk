# simpson's 1/3

from numpy import*
from math import*
def f(x):
  return 1/(1+x)
x0=eval(input("Enter lower limit\t"))
xn=eval(input("Enter upper limit\t"))
n=int(input("Enter how many subinervalue\t"))
h=(xn-x0)/n
d=zeros([2,n+1])
for i in range(n+1):
  d[0,i]=x0+i*h
  d[1,i]=f(x0+i*h)
for i in range(2):
  print("\n")
  for j in range(n+1):
      print("\t",round(d[i,j],4),end="")
sum=f(x0)+f(xn)
sum1=0
sum2=0
for i in range(1,n):
  if i%2==0:
     sum1=sum1+f(x0+i*h)
  else:
     sum2=sum2+f(x0+i*h)
area=(h/3)*(sum+2*sum1+4*sum2)
print("\n")
print('the area bounded by curve is',round(area,10))
